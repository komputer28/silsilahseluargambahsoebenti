            <div class="album-section">
                <h2 class="album-title">Album Foto Keluarga</h2>

                <div style="
                    text-align:center;
                    background:#fff3cd;
                    padding:20px;
                    border-radius:10px;
                    border:1px solid #ffeeba;
                    color:#856404;
                    font-size:18px;
                    margin-top:20px;
                ">
                    <strong>Maaf!</strong> Album foto sedang dalam perbaikan.
                    <br>Waktu saat ini: <strong id="realTimeClockAlbum"></strong>
                </div>
            </div>
        </div>

        <script>
        // JAM REALTIME UNTUK HALAMAN ALBUM
        function updateClockAlbum() {
            const now = new Date();

            const formatted = now.toLocaleString('id-ID', {
                weekday: 'long',
                day: 'numeric',
                month: 'long',
                year: 'numeric',
                hour: '2-digit',
                minute: '2-digit',
                second: '2-digit'
            });

            document.getElementById("realTimeClockAlbum").textContent = formatted;
        }

        setInterval(updateClockAlbum, 1000);
        updateClockAlbum();
        </script>

        <!-- TAB KOMENTAR -->
        <div id="comments" class="tab-content">

